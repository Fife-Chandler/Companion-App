<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Model.Families" table="families">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="familyName" column="familyName" type="string"/>
        <property name="address" column="address" type="string"/>
        <property name="city" column="city" type="string"/>
        <property name="st" column="st" type="string"/>
        <property name="zip" column="zip" type="string"/>
    </class>
    
    <class name="Model.District" table="district">
        <id name="id" type="int" column="id">
            <generator class="native"/>
        </id>
        <property name="memberID" column="memberID" type="int"/>
    </class>
</hibernate-mapping>
